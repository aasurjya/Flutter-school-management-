# Supabase Configuration
# Copy this file to .env and fill in your actual values

# Supabase Project URL (from Supabase Dashboard > Settings > API > Project URL)
SUPABASE_URL=https://your-project-ref.supabase.co

# Supabase Anonymous Key (from Supabase Dashboard > Settings > API > anon public)
SUPABASE_ANON_KEY=your-anon-key-here

# App Environment: development, staging, production
APP_ENV=development

# Show demo credentials on login screen (true/false)
SHOW_DEMO_CREDENTIALS=false

# Optional: Deep link redirect URL for OAuth
SUPABASE_REDIRECT_URL=

# Optional: Service role key (NEVER use in mobile/web frontend)
# SUPABASE_SERVICE_ROLE_KEY=
